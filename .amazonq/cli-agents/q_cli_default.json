{
  "name": "default",
  "description": "Default agent",
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md",
    "file://docs/**/*.md",
    "file://main.py",
    "file://src/**/*.py",
    "file://tests/**/*.py"
  ],
  "tools": ["*"],
  "allowedTools": ["execute_bash"],
  "toolsSettings": {
    "execute_bash": {
      "allowedCommands": [
        "grep",
        "find",
        "wc",
        "head",
        "tail",
        "cat",
        "diff",
        "git diff",
        "git log",
        "eslint",
        "pylint",
        "pytest",
        "rubocop",
        "ruff",
        "ruff format *",
        "source .venv/bin/activate"
      ]
    }
  },
  "useLegacyMcpJson": true
}
